{
  "_version": "1.49.0",
  "sap.app": {
    "id": "ns.artists",
    "type": "application",
    "title": "Morrowland Portal",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/festival/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://microphone"
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ns.artists.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ns.artists.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "ns.artists.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": "",
          "target": "TargetHome"
        },
        {
          "name": "RouteArtists",
          "pattern": "artists",
          "target": "TargetArtists"
        },
        {
          "name": "RouteDetail",
          "pattern": "artists/{artistID}",
          "target": "TargetDetail"
        },
        {
          "name": "RouteOrders",
          "pattern": "orders",
          "target": "TargetList"
        }
      ],
      "targets": {
        "TargetHome": {
          "viewName": "Home",
          "viewId": "Home"
        },
        "TargetArtists": {
          "viewName": "Artists",
          "viewId": "Artists"
        },
        "TargetDetail": {
          "viewName": "Detail",
          "viewId": "Detail"
        },
        "TargetList": {
          "viewName": "List",
          "viewId": "List"
        }
      }
    }
  }
}